{
  "application": {
    "name": "YourApp",
    "description": "Example description",
    "version": "0.2.0"
  },
  "server": {
    "host": "localhost",
    "port": 8000,
    "debug": false,
    "version": "/v1/"
  },
  "ratelimit": {
    "enabled": true,
    "headers": true
  },
  "security": {
    "audit.enabled": true,
    "ratelimit.enabled": true,
    "tracking.enabled": true
  },
  "path": {
    "configuration": "./configuration/",
    "templates": "./templates/",
    "static": "./templates/static/",
    "database": "./templates/static/database/",
    "logs": "./logs/",
    "init": {
      "audit": "./templates/static/database/audit.json",
      "index": "./templates/static/database/index.json",
      "transactions": "./templates/static/database/transactions.json",
      "users": "./templates/static/database/users.json",
      "wishes": "./templates/static/database/wishes.json"
    }
  }
}